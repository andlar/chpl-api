						"exec": [
							"const developerSchema =",
							"{",
							"  \"developers\": [",
							"    {",
							"      \"address\": {",
							"        \"addressId\": 0,",
							"        \"city\": \"string\",",
							"        \"country\": \"string\",",
							"        \"line1\": \"string\",",
							"        \"line2\": \"string\",",
							"        \"state\": \"string\",",
							"        \"zipcode\": \"string\"",
							"      },",
							"      \"contact\": {",
							"        \"contactId\": 0,",
							"        \"email\": \"string\",",
							"        \"fullName\": \"string\",",
							"        \"phoneNumber\": \"string\",",
							"        \"title\": \"string\"",
							"      },",
							"      \"deleted\": false,",
							"      \"developerCode\": \"string\",",
							"      \"developerId\": 0,",
							"      \"lastModifiedDate\": \"string\",",
							"      \"name\": \"string\",",
							"      \"selfDeveloper\": false,",
							"      \"status\": {",
							"        \"id\": 0,",
							"        \"status\": \"string\"",
							"      },",
							"      \"statusEvents\": [",
							"        {",
							"          \"developerId\": 0,",
							"          \"id\": 0,",
							"          \"reason\": \"string\",",
							"          \"status\": {",
							"            \"id\": 0,",
							"            \"status\": \"string\"",
							"          },",
							"          \"statusDate\": \"2020-10-09T17:08:51.022Z\"",
							"        }",
							"      ],",
							"      \"transparencyAttestations\": [",
							"        {",
							"          \"acbId\": 0,",
							"          \"acbName\": \"string\",",
							"          \"attestation\": {",
							"            \"removed\": false,",
							"            \"transparencyAttestation\": \"string\"",
							"          }",
							"        }",
							"      ],",
							"      \"website\": \"string\"",
							"    }",
							"  ]",
							"}",
							"var jsonResponse = pm.response.json();",
							"pm.test('developer schema is valid', function() {",
							"  pm.expect(tv4.validate(jsonResponse, developerSchema)).to.be.true;  ",
							"});",
							"",
							"pm.test(\"developer contact details do not include friendly name\", function () {",
							"    pm.expect(pm.response.text()).to.not.include(\"friendlyName\");",
							"});",
