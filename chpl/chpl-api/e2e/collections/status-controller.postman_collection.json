					"listen": "test",
					"script": {
						"id": "a8681d61-e31d-4da6-8f50-1d7793df6120",
						"exec": [
							"pm.test(\"API calls that run without API Key still work with valid API Keys should return 200 and response body should not be empty\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.response).not.eq(null);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
